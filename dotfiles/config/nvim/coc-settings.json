        "class": "\uf0e8",
        "interface": "\uf417",
        "text": "\u2663",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "module": "\uf530",
        "color": "\ue22b",
        "property": "\ue624",
        "field": "\uf6a6",
        "unit": "\uf475",
        "event": "\ufacd",
        "file": "\uf723",
        "folder": "\uf114",
        "snippet": "\ue60b",
        "typeParameter": "\uf728", "default": "\uf29c"
    },
    "list.source.outline.defaultOptions": ["--auto-preview"],
    "git.enableGutters": true,
